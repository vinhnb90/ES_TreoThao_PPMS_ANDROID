        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="16542052"
        android:textColor="@color/gsht_text_content"
        android:layout_marginLeft="5dp"
        android:layout_below="@+id/gsht_row_kh_tv_so_cto_treo"
        android:layout_toRightOf="@+id/gsht_row_kh_tv_tittle_cs_thao"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/gsht_row_kh_tv_tittle_cs_treo"
