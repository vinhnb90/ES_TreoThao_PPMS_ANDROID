        android:layout_height="wrap_content"
        android:text="CS treo: "
        android:textColor="@color/gsht_text_content"
        android:textStyle="bold"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/gsht_row_kh_tv_so_cto_treo"
        android:layout_toRightOf="@+id/gsht_row_kh_tv_cs_thao"
