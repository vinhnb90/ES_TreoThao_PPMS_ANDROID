<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape>
            <solid android:color="#9EA3A8" />

            <stroke android:width="2dp" android:color="#FFFFFF" />

        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:angle="-90" android:endColor="#076b09" android:startColor="#076b09" />

            <stroke android:width="2dp" android:color="#FFFFFF" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="-90" android:endColor="#FB8246" android:startColor="#fc9765" />
            <!--<gradient android:angle="-90" android:endColor="#3cbc3f" android:startColor="#3cbc3f" />-->

            <stroke android:width="2dp" android:color="#FFFFFF" />

        </shape>
    </item>

</selector>